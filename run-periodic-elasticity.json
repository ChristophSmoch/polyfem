{
    "mesh": "square.obj",
    "n_refs": 3,
    "discr_order": 2,
    "scalar_formulation": "LinearElasticity",
    "solver_type": "Eigen::PardisoLU",
    "problem": "GenericTensor",
    "export":
    {
        "stiffness_mat": "A.mat",
        "surface": true,
        "material_params": true
    },
    "params": {
